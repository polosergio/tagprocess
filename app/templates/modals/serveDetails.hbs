<div class="container-fluid">
	<a href="#" class="openList"><span class="glyphicon glyphicon-arrow-left"></span></a>
{{#if comment}}
	<div class="row">
		<div class="table-responsive">
			<table class="table table-bordered table-condensed table-hover table-striped">
				<thead>
					<tr>
						<th>Date</th>
						<th>Comment</th>
						<th>Latitude</th>
						<th>Longitude</th>
					</tr>
				</thead>
				<tbody>
					<tr>
						<td>{{date}}</td>
						<td>{{comment}}</td>
						<td>{{latitude}}</td>
						<td>{{longitude}}</td>
					</tr>
				</tbody>
			</table>
		</div>
	</div>
	<div class="row">
		<img src="{{street}}" class="img-responsive col-md-6">
		<img src="{{satellite}}" class="img-responsive col-md-6">
	</div>
	<div class="row">
	    {{#parseImageOrVideo image 'img-responsive col-md-12'}}{{/parseImageOrVideo}}
	</div>
{{else}}
	<div class="row">
		<div class="col-md-6">
			<div class="table-responsive">
				<table class="table table-bordered table-condensed table-hover table-striped">
					<tbody>
						<tr>
							<th>Date</th>
							<td>{{timestamp}}</td>
						</tr>
						<tr>
							<th>GPS Location</th>
							<td>{{latitude}}, {{longitude}}</td>
						</tr>
						<tr>
							<th>Served On</th>
							<td>
								<div class="col-md-10 col-xs-10 noOverflow">
									{{served_person}}
								</div>
								{{#if admin}}
									<form class="form-inline col-md-10 col-xs-10 hide editServeForm" role="form">
										<div class="form-group col-md-8 col-xs-8">
											<div class="input-group">
												<label class="sr-only" for="served_form">{{served_person}}</label>
												<input class="form-control" type="text" name="served_person" id="served_person" value="{{served_person}}">
											</div>
										</div>
										<button type="submit" class="btn btn-default col-md-2 col-xs-2">
											<span class="glyphicon glyphicon-floppy-disk"></span>
										</button>
										<button type="button" class="btn btn-default toggle col-md-2 col-xs-2">
											<span class="glyphicon glyphicon-remove"></span>
										</button>
									</form>
									<a href="#" class="toggle col-md-2 col-xs-2 text-right"><span class="glyphicon glyphicon-pencil" style="color: goldenrod;"></span></a>
								{{/if}}
							</td>
						</tr>
						<tr>
							<th>Gender</th>
							<td>
								<div class="col-md-10 col-xs-10 noOverflow">
									{{gender}}
								</div>
								{{#if admin}}
									<form class="form-inline col-md-10 col-xs-10 hide editServeForm" role="form">
										<div class="form-group col-md-8 col-xs-8">
											<div class="input-group">
												<label class="sr-only" for="gender">{{gender}}</label>
												<input class="form-control" type="text" name="gender" id="gender" value="{{gender}}">
											</div>
										</div>
										<button type="submit" class="btn btn-default col-md-2 col-xs-2">
											<span class="glyphicon glyphicon-floppy-disk"></span>
										</button>
										<button type="button" class="btn btn-default toggle col-md-2 col-xs-2">
											<span class="glyphicon glyphicon-remove"></span>
										</button>
									</form>
									<a href="#" class="toggle col-md-2 col-xs-2 text-right"><span class="glyphicon glyphicon-pencil" style="color: goldenrod;"></span></a>
								{{/if}}
							</td>
						</tr>	
						<tr>
							<th>Age</th>
							<td>
								<div class="col-md-10 col-xs-10 noOverflow">
									{{age}}
								</div>
								{{#if admin}}
									<form class="form-inline col-md-10 col-xs-10 hide editServeForm" role="form">
										<div class="form-group col-md-8 col-xs-8">
											<div class="input-group">
												<label class="sr-only" for="age">{{age}}</label>
												<input class="form-control" type="text" name="age" id="age" value="{{age}}">
											</div>
										</div>
										<button type="submit" class="btn btn-default col-md-2 col-xs-2">
											<span class="glyphicon glyphicon-floppy-disk"></span>
										</button>
										<button type="button" class="btn btn-default toggle col-md-2 col-xs-2">
											<span class="glyphicon glyphicon-remove"></span>
										</button>
									</form>
									<a href="#" class="toggle col-md-2 col-xs-2 text-right"><span class="glyphicon glyphicon-pencil" style="color: goldenrod;"></span></a>
								{{/if}}
							</td>
						</tr>
						<tr>
							<th>Race</th>
							<td>
								<div class="col-md-10 col-xs-10 noOverflow">
									{{race}}
								</div>
								{{#if admin}}
									<form class="form-inline col-md-10 col-xs-10 hide editServeForm" role="form">
										<div class="form-group col-md-8 col-xs-8">
											<div class="input-group">
												<label class="sr-only" for="race">{{race}}</label>
												<input class="form-control" type="text" name="race" id="race" value="{{race}}">
											</div>
										</div>
										<button type="submit" class="btn btn-default col-md-2 col-xs-2">
											<span class="glyphicon glyphicon-floppy-disk"></span>
										</button>
										<button type="button" class="btn btn-default toggle col-md-2 col-xs-2">
											<span class="glyphicon glyphicon-remove"></span>
										</button>
									</form>
									<a href="#" class="toggle col-md-2 col-xs-2 text-right"><span class="glyphicon glyphicon-pencil" style="color: goldenrod;"></span></a>
								{{/if}}
							</td>
						</tr>
						<tr>
							<th>Height</th>
							<td>
								<div class="col-md-10 col-xs-10 noOverflow">
									{{height}}
								</div>
								{{#if admin}}
									<form class="form-inline col-md-10 col-xs-10 hide editServeForm" role="form">
										<div class="form-group col-md-8 col-xs-8">
											<div class="input-group">
												<label class="sr-only" for="height">{{height}}</label>
												<input class="form-control" type="text" name="height" id="height" value="{{height}}">
											</div>
										</div>
										<button type="submit" class="btn btn-default col-md-2 col-xs-2">
											<span class="glyphicon glyphicon-floppy-disk"></span>
										</button>
										<button type="button" class="btn btn-default toggle col-md-2 col-xs-2">
											<span class="glyphicon glyphicon-remove"></span>
										</button>
									</form>
									<a href="#" class="toggle col-md-2 col-xs-2 text-right"><span class="glyphicon glyphicon-pencil" style="color: goldenrod;"></span></a>
								{{/if}}
							</td>
						</tr>
						<tr>
							<th>Weight</th>
							<td>
								<div class="col-md-10 col-xs-10 noOverflow">
									{{weight}}
								</div>
								{{#if admin}}
									<form class="form-inline col-md-10 col-xs-10 hide editServeForm" role="form">
										<div class="form-group col-md-8 col-xs-8">
											<div class="input-group">
