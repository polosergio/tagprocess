<div class="container-fluid">
	<div class="row">
		<div class="col-md-2 sidebar"></div>
		<div class="col-md-10">
			<form class="form-horizontal" data-url="/tagproc/api/employee">
				<div class="form-group">
					<label for="employee_username" class="col-sm-2 control-label">Username</label>
					<div class="col-sm-10">
						<input type="text" class="form-control" id="employee_username" name="employee_username" placeholder="Username" required>
					</div>
				</div>
				<div class="form-group">
					<label for="employee_password" class="col-sm-2 control-label">Password</label>
					<div class="col-sm-10">
						<input type="text" class="form-control" id="employee_password" name="employee_password" placeholder="Password" required>
					</div>
				</div>
				<div class="form-group">
					<label for="employee_name" class="col-sm-2 control-label">Employee Name</label>
					<div class="col-sm-10">
						<input type="tel" class="form-control" id="employee_name" name="employee_name" placeholder="Employee's Name" required>
					</div>
				</div>
				<div class="form-group">
					<label for="employee_phone" class="col-sm-2 control-label">Phone #</label>
					<div class="col-sm-10">
						<input type="tel" class="form-control" id="employee_phone" name="employee_phone" placeholder="Phone #">
					</div>
				</div>
				<div class="form-group">
					<label for="employee_fax" class="col-sm-2 control-label">Fax #</label>
					<div class="col-sm-10">
						<input type="tel" class="form-control" id="employee_fax" name="employee_fax" placeholder="Fax #">
					</div>
				</div>
				<div class="form-group">
					<label for="employee_email" class="col-sm-2 control-label">E-mail</label>
					<div class="col-sm-10">
						<input type="text" class="form-control" id="employee_email" name="employee_email" placeholder="E-mail" required>
					</div>
				</div>
				<div class="form-group">
					<label for="employee_address" class="col-sm-2 control-label">Address</label>
					<div class="col-sm-10">
						<input type="text" class="form-control" id="employee_address" name="employee_address" placeholder="Employee's Address" required>
					</div>
				</div>
				<div class="form-group">
					<label for="employee_city" class="col-sm-2 control-label">City</label>
					<div class="col-sm-10">
						<input type="text" class="form-control" id="employee_city" name="employee_city" placeholder="City" required>
					</div>
				</div>
				<div class="form-group">
					<label for="employee_state" class="col-sm-2 control-label">State</label>
					<div class="col-sm-10">
						<input type="text" class="form-control" id="employee_state" name="employee_state" placeholder="State" required>
					</div>
				</div>
				<div class="form-group">
					<label for="employee_zip" class="col-sm-2 control-label">Zip</label>
					<div class="col-sm-10">
						<input type="text" class="form-control" id="employee_zip" name="employee_zip" placeholder="Zip" required>
					</div>
				</div>
				<div class="alert alert-danger hide col-sm-offset-2 col-sm-10"></div>
				<div class="form-group">
					<div class="col-sm-offset-2 col-sm-10">
						<button type="submit" class="btn btn-default btn-block">Submit</button>
					</div>
				</div>
			</form>
		</div>
	</div>
</div>